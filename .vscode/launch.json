  "version": "0.2.0",
  "configurations": [
    {
      "command": "cds watch --profile hybrid --port 4004",
      "name": "cds watch --profile hybrid --port 4004",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:4004",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Run TSI-2",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env2",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/KNPL-TSI/TSI\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}